                            <div class="form-group input-group-sm" id="div_tarjeta">
                                <label for="txt_tarjeta">Tarjeta</label>
                                <input type="text" class="form-control nombre limpiar input cuenta requerido" id="txt_tarjeta" name="txt_tarjeta" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                            </div>

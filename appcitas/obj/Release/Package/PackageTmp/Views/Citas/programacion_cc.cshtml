                <li role="presentation" class="hide"><a href="#nueva_cita" aria-controls="nueva_cita" role="tab" data-toggle="tab">Datos generales</a></li>
                <li role="presentation" class="hide"><a href="#nueva_cita_calendario" aria-controls="nueva_cita_calendario" role="tab" data-toggle="tab">Asignacion de cita</a></li>
                <li role="presentation" class="hide"><a href="#nueva_cita_razon" aria-controls="nueva_cita_razon" role="tab" data-toggle="tab">Razon de cancelacion</a></li>
                <li role="presentation" class="hide"><a href="#vista_previa_cita" aria-controls="vista_previa_cita" role="tab" data-toggle="tab">Confirmacion cita</a></li>
                <li role="presentation" class="hide"><a href="#motorDeRetencionesAnualidad" aria-controls="motorDeRetencionesAnualidad" role="tab" data-toggle="tab">Motor de Retenciones</a></li>
                <li role="presentation" class="hide"><a href="#motorDeRetencionesReversion" aria-controls="motorDeRetencionesReversion" role="tab" data-toggle="tab">Motor de Retenciones</a></li>
                <li role="presentation" class="hide"><a href="#motorDeRetencionesTasas" aria-controls="motorDeRetencionesTasas" role="tab" data-toggle="tab">Motor de Retenciones</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content bac-table-panelx">
                <div role="tabpanel" class="tab-pane fade" id="inicio">
                    <div class="row no-margin">
                        <div class="col-sm-6 col-sm-offset-3">
                            <div class="form-group input-group custom-search-form input-group-sm">
                                <input type="text" class="form-control" data-provide="typeahead" id="busqueda" placeholder="Buscar nombre, correo electronico o numero de identidad" autocomplete="off" data-original-title="" title="">
                                <span class="input-group-btn">
                                    <button class="btn btn-info" type="button" id="btn_buscarid" name="btn_buscarid">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div><!-- /input-group -->
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="resultado_busqueda_ninguno">
                    <div class="row" style="padding: 15px;">
                        <div class="col-xs-12 text-center">
                            <span>Ninguna coincidencia encontrada con </span>
                            <b>'<span id="texto_busqueda"></span>'</b>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-xs-6 text-right">
                            <button class="btn btn-sm btn-default" id="btn_inicio" onClick='buscarDeNuevo()'>
                                <span>Buscar de nuevo </span>
                                <i class="fa fa-search"></i>
                            </button>
                        </div>
                        <div class="col-xs-6 text-left">
                            <button class="btn btn-sm btn-primary" id="btn_nueva_cita" onClick='nuevaCita()'>
                                <i class="fa fa-plus"> </i>
                                <span>Nueva cita</span>
                            </button>
                        </div>
                    </div>
                </div>

                <div role="tabpanel" class="tab-pane fade" id="resultado_busqueda_encontrado">
                    <div class="row bac-container-table-panel">
                        <div class="col-xs-12 text-center" style="padding-top: 15px;" ">
                            <span>Coincidencias para </span>
                            <b>'<span id="texto_busqueda_encontrado"></span>'</b>
                        </div>
                        <div class="col-xs-12 bac-table-panel">
                            <table id="tableCitas" class="list-table table table-bordered">
                                <thead>
                                    <tr>
                                        <th>No.</th>
                                        <th>Codigo de cita</th>
                                        <th>Identidad</th>
                                        <th>Nombre</th>
                                        <th>Correo Electronico</th>
                                        <th>Fecha cita</th>
                                        <th>Centro de fidelizacion</th>
                                        <th>Tramite</th>
                                        <th>Estado</th>
                                        <th>Acciones</th>
                                        <th hidden="hidden"></th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                    <div class="row" style="margin: 0; margin-top: 20px;">
                        <div class="col-xs-6 text-right">
                            <button class="btn btn-sm btn-default" id="btn_inicio" onClick='buscarDeNuevo()'>
                                <span>Buscar de nuevo </span>
                                <i class="fa fa-search"></i>
                            </button>
                        </div>
                        <div class="col-xs-6 text-left">
                            <button class="btn btn-sm btn-primary" id="btn_nueva_cita" onClick='nuevaCita()'>
                                <i class="fa fa-plus"> </i>
                                <span>Nueva cita</span>
                            </button>
                        </div>
                    </div>
                </div>



@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="box bac">
            <div class="box-header">
                <h3 class="titulo">Dashboard consolidado</h3>
            </div>
            <div class="box-body">
                <div class="row nomargin">
                    <div class="form-group input-group-sm date col-md-2">
                        <label for="txt_fecha1">Fecha Inicio</label>
                        <div class='input-group input-group-sm date' id="fecha1">
                            <span class="input-group-addon">
                                <span class="fa fa-calendar"></span>
                            </span>
                            <input type='text' class="form-control limpiar requerido" data-requerido="true" id="txt_fecha1" readonly="" attr-message="Es un filtro requerido!">
                            <div class="validation-error"><p></p></div>
                        </div>
                    </div>
                    <div class="form-group input-group-sm date col-md-2">
                        <label for="txt_fecha2">Fecha Final</label>
                        <div class='input-group input-group-sm date' id="fecha2">
                            <span class="input-group-addon">
                                <span class="fa fa-calendar"></span>
                            </span>
                            <input type='text' class="form-control limpiar requerido" data-requerido="true" id="txt_fecha2" readonly="" attr-message="Es un filtro requerido!">
                            <div class="validation-error"><p></p></div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Centro de fidelización</label>
                        <select class="form-control requerido" data-requerido="true" id="cmb_sucursal" attr-message="Es un campo requerido!">
                            <option value="-1">Ninguna</option>
                        </select>
                        <div class="validation-error"><p></p></div>
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Ejecutivo de atención</label>
                        <select class="form-control requerido" data-requerido="true" id="cmb_cubiculo" attr-message="Es un campo requerido!">
                            <option value="0">Todos</option>
                        </select>
                        <div class="validation-error"><p></p></div>
                    </div>
                    <div class="col-md-2">
                        <label class="control-label">Tipo de atención</label>
                        <select class="form-control requerido" data-requerido="true" id="cmb_tipo_atencion" attr-message="Es un campo requerido!">
                            <option value="-1">Ninguna</option>
                        </select>
                        <div class="validation-error"><p></p></div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-2 col-md-offset-5">
                        <button type="button" class="btn btn-md btn-primary" id="btn_crear_reporte">Generar Reporte</button>
                    </div>
                </div>
                <div class="row bac-container-table-panel">
                    <div class="col-xs-12 bac-table-panel">
                        <table class="table list-table table-bordered" id="tableCitasDashboard1">
                            <thead>
                                <tr class="">
                                    <th>Nro</th>
                                    <th>Centro de fidelización</th>
                                    <th>Tipo de Atención</th>
                                    <th>Ejecutivo de Atención</th>
                                    <th>Tiempo promedio espera (minutos)</th>
                                    <th>Tiempo promedio atención (minutos)</th>
                                    <th>Tiempo total de la permanencia (minutos)</th>
                                    <th>Clientes atendidos</th>
                                    <th>Citas emitidos</th>
                                    <th>Citas abandonadas</th>
                                    <th>Días laborados por ejecutivos</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Scripts/js/dashboard_consolidado.js")
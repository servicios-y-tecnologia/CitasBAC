
@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <div class="box bac">
            <div class="box-header">
                <h3 class="titulo">Dashboard Walkin</h3>
            </div>
            <div class="box-body">
                <div class="row nomargin">
                    <div class="form-group input-group-sm date col-md-2">
                        <label for="txt_fecha1">Fecha Inicio</label>
                        <div class='input-group input-group-sm date' id="fecha1">
                            <span class="input-group-addon">
                                <span class="fa fa-calendar"></span>
                            </span>
                            <input type='text' class="form-control limpiar requerido" data-requerido="true" id="txt_fecha1" readonly="" attr-message="Es un filtro requerido!">
                            <div class="validation-error"><p></p></div>
                        </div>
                    </div>
                    <div class="form-group input-group-sm date col-md-2">
                        <label for="txt_fecha2">Fecha Final</label>
                        <div class='input-group input-group-sm date' id="fecha2">
                            <span class="input-group-addon">
                                <span class="fa fa-calendar"></span>
                            </span>
                            <input type='text' class="form-control limpiar requerido" data-requerido="true" id="txt_fecha2" readonly="" attr-message="Es un filtro requerido!">
                            <div class="validation-error"><p></p></div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Centro de fidelización</label>
                        <select class="form-control requerido" data-requerido="true" id="cmb_sucursal" attr-message="Es un campo requerido!">
                            <option value="-1">Ninguna</option>
                        </select>
                        <div class="validation-error"><p></p></div>
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Ejecutivo de atención</label>
                        <select class="form-control requerido" data-requerido="true" id="cmb_cubiculo" attr-message="Es un campo requerido!">
                            <option value="0">Todos</option>
                        </select>
                        <div class="validation-error"><p></p></div>
                    </div>
                    <div class="col-md-2">
                        <label class="control-label">Estados</label>
                        <select class="form-control requerido" data-requerido="true" id="cod_estado" attr-message="Es un campo requerido!">
                            <option value="-1">Ninguna</option>
                        </select>
                        <div class="validation-error"><p></p></div>
                    </div>
                </div>
                <div class="row nomargin" style="margin-bottom: 25px !important; margin-top: 10px !important;">
                    <div class="col-xs-12 text-center">
                        <button type="button" class="btn btn-md btn-primary" id="btn_crear_reporte_dashboard1">Generar reporte</button>
                    </div>
                </div>
                <div class="row bac-container-table-panel">
                    <div class="col-xs-12 bac-table-panel">
                        <table class="table list-table table-bordered" id="tableCitasDashboard1">
                            <thead>
                                <tr class="">
                                    <th>Nro</th>
                                    <th width="15%">Ticket</th>
                                    <th width="15%">Ejecutivo de atención</th>
                                    <th width="15%">Ejecutivo en posición</th>
                                    <th width="15%">Día</th>
                                    <th width="15%">Sucursal</th>
                                    <th width="15%">Fecha</th>
                                    <th width="15%">Hora de Llegada</th>
                                    <th width="10%">Hora inicio</th>
                                    <th width="10%">Hora fin</th>
                                    <th width="10%">Tiempo atención (minutos)</th>
                                    <th width="10%">Tiempo espera (minutos)</th>
                                    <th width="15%">Resolución</th>
                                    <th width="10%">Nombre cliente</th>
                                    <th width="10%">Cuenta</th>
                                    <th width="10%">Tarjetas</th>
                                    <th width="10%">Segmento</th>
                                    <th width="10%">Marca</th>
                                    <th width="10%">Familia</th>
                                    <th width="10%">Tramite</th>
                                    <th width="10%">Razones</th>
                                    <th width="10%">Herramientas</th>
                                    <th width="10%">Estado</th>
                                    <th width="10%">Origen</th>
                                    <th hidden="hidden"></th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/Scripts/js/dashboard2.js")

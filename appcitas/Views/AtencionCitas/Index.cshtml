                                            <button id="btn-wal-refresh" type="button" class="btn btn-secondary"><span class="glyphicon glyphicon-refresh" aria-hidden="true"></span></button>
                                            <button id="btn-wal-next" type="button" class="btn btn-secondary"><span class="glyphicon glyphicon-step-forward" aria-hidden="true"></span></button>

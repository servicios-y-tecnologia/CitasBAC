                    <div class="box bac">
                        <div class="box-header">
                            <h3 class="titulo">Citas Atendidas</h3>
                        </div>
                        <div class="box-body">
                            <div class="row">

                                <table class="list-table table table-bordered">
                                    <thead>
                                        <tr>
                                            <th class="text-center">Walkin</th>
                                            <th class="text-center">Program</th>


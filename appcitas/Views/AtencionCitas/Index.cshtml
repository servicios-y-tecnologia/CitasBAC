                                                    <input type="text" class="form-control limpiar input cuentaFormato modal_requerido" data-requerido="true" attr-message="Es un campo requerido!" id="txt_tarjeta" name="txt_tarjeta" placeholder="****-****-****-1234" maxlength="50" autocomplete="off" disabled>
                                                    <input type="text" class="form-control limpiar input modal_requerido hide" data-requerido="false" attr-message="Es un campo requerido!" id="txt_tarjeta_formato" placeholder="9998984" maxlength="50" autocomplete="off" readonly="" disabled>
                                                </div>
                                            </div>
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_cel">
                                                    <label for="txt_cuenta">No. Cuenta</label>
                                                    @*<input type="text" class="form-control nombre limpiar input requerido" id="txt_cuenta" name="txt_cuenta" placeholder="N/D" maxlength="50" autocomplete="off" disabled>*@
                                                    <input type="text" class="form-control limpiar input cuenta cuentaFormato modal_requerido" data-requerido="true" attr-message="Es un campo requerido!" id="txt_cuenta" placeholder="9998984" maxlength="50" autocomplete="off" disabled>
                                                    <input type="text" class="form-control limpiar input modal_requerido hide" data-requerido="false" attr-message="Es un campo requerido!" id="txt_cuenta_formato" placeholder="9998984" maxlength="50" autocomplete="off" readonly="" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tarjeta">
                                                    <label for="txt_marca">Marca</label>
                                                    <input type="text" class="form-control nombre limpiar input cuenta requerido" id="txt_marca" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tarjeta">
                                                    <label for="txt_producto">Producto</label>
                                                    <input type="text" class="form-control nombre limpiar input cuenta requerido" id="txt_producto" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tarjeta">
                                                    <label for="txt_familia">Familia</label>
                                                    <input type="text" class="form-control nombre limpiar input cuenta requerido" id="txt_familia" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_segmento">
                                                    <label for="txt_segmento">Segmento</label>
                                                    @*<input type="text" class="form-control nombre limpiar input requerido" id="txt_segmento" name="txt_segmento" placeholder="N/D" maxlength="50" autocomplete="off" disabled>*@
                                                    <select id="cod_segmento" class="requerido" data-requerido="true" attr-message="Es un campo requerido!" disabled></select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_sucursal">
                                                    <label for="txt_sucursal">Sucursal</label>
                                                    <input type="text" class="form-control nombre limpiar input requerido" id="txt_sucursal" name="txt_sucursal" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tramite">
                                                    <label for="txt_tramite">Tramite</label>
                                                    <input type="text" class="form-control nombre limpiar input requerido" id="txt_tramite" name="txt_tramite" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_fech">
                                                    <label for="txt_fecha">Fecha</label>
                                                    <input type="text" class="form-control nombre limpiar input requerido" id="txt_fecha" name="txt_fecha" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_hora">
                                                    <label for="txt_hora">Hora</label>
                                                    <input type="text" class="form-control nombre limpiar input requerido" id="txt_hora_vista" name="txt_hora_vista" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <button class="btn btn-md btn-success pull-right hidden" id="btn_guardarCuenta">
                                                <span> Guardar </span>
                                                <i class="fa fa-save"> </i>
                                            </button>
                                        </div>

                                        <div class="row no-margin">
                                            <button class="btn btn-md btn-success pull-right hidden" id="btn_guardarTarjeta">
                                                <span> Guardar </span>
                                                <i class="fa fa-save"> </i>
                                            </button>
                                        </div>

                                    </div> 

                                    <div role="tabpanel" class="tab-pane fade" id="nueva_cita">
                                        <!-- Razones -->

                                        


                                        <div class="row margin-bottom">

                                            


                                            <div class="col-sm-12 bottom-space-sm">
                                                <div class="form-group input-group-sm" id="div_razon">


                                                    


                                                    @*<label for="guardarRazon">Agregar Razón</label>*@
                                                    <button class="btn btn-md btn-success pull-right" id="guardarRazon">
                                                        <i class="fa fa-plus"> Agregar Razón</i>
                                                        <span></span>
                                                    </button>
                                                </div>

                                                <div class="col-sm-8">
                                                    <div class="form-group input-group-sm hidden" id="campocitarazon">
                                                        <label>Cita Razón: </label>
                                                        <input type="text" id="citarazon" size="12" disabled style="border-style: none; background-color:  ActiveBorder; text-align: center; font-weight: bold;">

                                                    </div>
                                                </div>


                                            </div>
                                        </div>


                                        


                                        <div class="row">

                                            <div class="col-sm-4 bottom-space-sm">
                                                <div class="form-group input-group-sm" id="div_tipo_razon">
                                                    <label for="cod_tipo_razon">Tipo de Razon</label>
                                                    <select id="cod_tipo_razon" class="requerido" data-requerido="true" attr-message="Es un campo requerido!"></select>
                                                    <div class="validation-error"><p></p></div>
                                                </div>
                                            </div>
                                            <div class="col-sm-4">
                                                <div class="form-group input-group-sm" id="div_razon">
                                                    <label for="cod_razonp">Razon</label>
                                                    <select id="cod_razonp" class="requerido" data-requerido="true" attr-message="Es un campo requerido!"></select>
                                                    <div class="validation-error"><p></p></div>
                                                </div>
                                            </div>
                                            <div class="col-sm-4 hide" id="listadoExtraContainer">
                                                <div class="form-group input-group-sm" id="div_listado">
                                                    <label for="cod_listado_extra" id="label_cod_listado_extra">Listado Extra</label>
                                                    <select id="cod_listado_extra" class="requerido" data-requerido="true" attr-message="Es un campo requerido!"></select>
                                                    <div class="validation-error"><p></p></div>
                                                </div>
                                            </div>

                                        </div>

                                        <div class="row margin-bottom">
                                            <div class="box-body">
                                                <table id="tableRazones" class="list-table table table-bordered">
                                                    <thead>
                                                        <tr>
                                                            <th>No.</th>
                                                            <th>Tipo de razon</th>
                                                            <th>Razon</th>
                                                            <th>Origen</th>
                                                            <th></th>
                                                            <th>Acciones</th>
                                                            <th hidden="hidden"></th>
                                                        </tr>
                                                    </thead>
                                                </table>
                                            </div>
                                        </div>                                                                              
                                        
                                        <!-- /Razones -->

                                        <div class="bac-table-panel2"></div>

                                        <!-- Herramientas -->
                                        <div class="row margin-bottom">
                                            <div class="col-sm-12 bottom-space-sm">
                                                <div class="form-group input-group-sm">                                                    
                                                    <button class="btn btn-md btn-success pull-right" id="guardarHerramienta">
                                                        <i class="fa fa-plus"> Agregar Herramienta</i>
                                                        <span></span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row">
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label for="cmbHerramientas">Herramientas</label>
                                                    <select id="cmbHerramientas" class="requerido cmbHerramientas"></select>
                                                    <div class="validation-error"><p></p></div>
                                                </div>
                                            </div>
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label for="txtObsHerramienta">Observación</label>
                                                    <textarea id="txtObsHerramienta" class="form-control" rows="3" placeholder="Ingrese alguna observación..."></textarea>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row margin-bottom">
                                            <div class="box-body">
                                                <table id="tableHerramientas" class="list-table table table-bordered">
                                                    <thead>
                                                        <tr class="text-center">
                                                            <th>Herramientas</th>
                                                            <th>Observación</th>
                                                            <th>Acciones</th>
                                                            <th hidden="hidden"></th>
                                                        </tr>
                                                    </thead>
                                                </table>
                                            </div>
                                        </div>

                                        <!-- /Herramientas -->
                                        
                                        <div class="bac-table-panel2 margin-bottom"></div>

                                        <div class="row margin-bottom">                                            
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label for="txt_observacion">Comentario</label>
                                                    <textarea id="txt_observacion" class="form-control" rows="3" placeholder="Ingrese algun comentario sobre la cita..."></textarea>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row margin-bottom">
                                            <div class="col-xs-12">
                                                <div class="form-group">
                                                    <label for="cmb_resultado">Resultado</label>
                                                    <select class="requerido" id="cmb_resultado" attr-message="Es un campo requerido!"></select>
                                                    <div class="validation-error"><p></p></div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row margin-bottom">
                                            <div class="col-md-12">
                                                <button class="btn btn-md btn-success pull-right" onclick='guardarCita(event)'>
                                                    <i class="fa fa-check-square-o"> </i>
                                                    <span> Finalizar</span>
                                                </button>
                                            </div>
                                        </div>
                                    </div>

                                    <div role="tabpanel" class="tab-pane fade" id="mas_tarjetas">
                                        <div class="row margin-bottom" style="overflow:auto">
                                            <div class="col-md-12">
                                                    <table id="tableTarjetas" class="list-table table table-bordered">
                                                        <thead>
                                                            <tr>
                                                                <th>#</th>
                                                                <th>No. Cuenta</th>
                                                                <th>No. Tarjeta</th>
                                                                <th>Segmento</th>
                                                                <th>Producto</th>
                                                                <th>Acciones</th>
                                                            </tr>
                                                        </thead>
                                                    </table>
                                            </div>                                            
                                        </div>

                                        <div class="row margin-bottom">
                                            <div class="col-sm-12 bottom-space-sm">
                                                <div class="form-group input-group-sm">                                                    
                                                    <button class="btn btn-md btn-success pull-right" id="btnNuevaTarjeta">
                                                        <i class="fa fa-plus"> Agregar Nueva Tarjeta</i>
                                                        <span></span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-4">
            <div id="contenedor_timer" class="row hidden">
                <div class="col-md-12">
                    <div class="box bac">
                        <div class="box-header">
                            <h3 class="titulo">Tiempo</h3>
                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div id="TimerCitas"></div>
                                </div>
                                <div class="col-md-12">
                                    <p class="text-center">
                                        <strong>Tiempos de tolerancia</strong>
                                    </p>
                                    <div class="progress-group hidden">
                                        <span class="progress-text">Duración Cita</span>
                                        <span class="progress-number"><b>24 Min</b>/30 Min</span>

                                        <div class="progress sm">
                                            <div class="progress-bar progress-bar-green" style="width: 80%"></div>
                                        </div>
                                    </div>
                                    <!-- /.progress-group -->
                                    <div class="progress-group">
                                        <span class="progress-text">Alerta Previa</span>
                                        <span id="lbl_alertaPrevia" class="progress-number"><b>20 Min</b>/25 Min</span>

                                        <div class="progress sm">
                                            <div id="bar_alertaPrevia" class="progress-bar progress-bar-yellow" style="width: 100%"></div>
                                        </div>
                                    </div>                                  

                                    <!-- /.progress-group -->
                                    <div class="progress-group">
                                        <span class="progress-text">Tolerancia a Finalización</span>
                                        <span id="lbl_Torelancia" class="progress-number"><b>5 Min</b>/10 Min</span>

                                        <div class="progress sm">
                                            <div id="bar_tolerancia" class="progress-bar progress-bar-red" style="width: 100%"></div>
                                        </div>
                                    </div>
                                    <!-- /.progress-group -->  
                                    
                                    <div id="contenedor_tiempomuerto" class="progress-group">
                                        <span class="progress-text">Tiempo Muerto</span>
                                        <span id="lbl_tiempoMuerto" class="progress-number"><b>2 Min</b>/15 Min</span>

                                        <div class="progress sm">
                                            <div id="bar_tiempoMuerto" class="progress-bar progress-bar-striped" style="width: 100%"></div>
                                        </div>
                                    </div>
                                    <!-- /.progress-group -->                                                                  
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="contenedor_mensaje_timer" class="row hidden">
                <div id="titleMessageTimerOut" class="alert alert-danger" role="alert"></div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="box bac">
                        <div class="box-header">
                            <h3 class="titulo">Programadas</h3>

                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="col-md-12">
                                        <div id="message_programa_proccess" class="text-center hidden">
                                            <a href="javascript:void(0);" class="list-group-item text-center list-group-item-vacio"><span id="spanProgramadas" class="label label-warning">Cita en proceso...</span></a>
                                        </div>
                                    </div>
                                    <div id="list_citas" class="list-group">
                                        <a href="#contenedor_principal" class="list-group-item text-center list-group-item-vacio"><span class="label label-warning">No hay citas que mostrar!</span></a>
                                    </div>                                    
                                </div>

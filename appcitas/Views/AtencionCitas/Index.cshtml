            <div class="row">
                <div class="col-md-12">
                    <div class="box bac">
                        <div class="box-header">
                            <h3 class="titulo">Citas del Día</h3>
                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div id="list_citas_dia" class="list-group">
                                        <a href="#contenedor_principal" class="list-group-item text-center list-group-item-vacio"><span class="label label-warning">No hay citas que mostrar!</span></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>



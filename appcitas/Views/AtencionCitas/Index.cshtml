                                    <div class="text-center">
                                        <div class="btn-group text-center" role="group" aria-label="Basic example">                                            
                                            <button id="btn-prog-refresh" type="button" class="btn btn-secondary"><span class="glyphicon glyphicon-refresh" aria-hidden="true"></span></button>
                                            <button id="btn-prog-next" type="button" class="btn btn-secondary"><span class="glyphicon glyphicon-step-forward" aria-hidden="true"></span></button>
                                        </div>
                                    </div>
                                </div>*@
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="box bac">
                        <div class="box-header">
                            <h3 class="titulo">Walkin</h3>
                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <div id="message_walkin_proccess" class="hidden text-center">                                        
                                        <a href="javascript:void(0);" class="list-group-item text-center list-group-item-vacio"><span id="spanWalkin" class="label label-warning">Cita en proceso...</span></a>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div id="list_citas_walkin" class="list-group">
                                        <a href="#contenedor_principal" class="list-group-item text-center list-group-item-vacio"><span class="label label-warning">No hay citas que mostrar!</span></a>
                                    </div>                                    
                                </div>
                                @*<div id="controles_walkin1" class="col-md-12 hidden">
                                    <div class="text-center">

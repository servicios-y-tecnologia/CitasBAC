
@{
    ViewBag.Title = "Atención de Citas";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-md-3 col-md-offset-3 margin-bottom">
        <label class="control-label">Sucursales</label>
        <select class="form-control" id="cmb_sucursal" attr-message="Es un campo requerido!">
            <option value="-1">Ninguna</option>
        </select>
        <div class="validation-error"><p></p></div>
    </div>

    <div class="col-md-3">
        <label class="control-label">Cubiculo</label>
        <select class="form-control" id="cmb_cubiculo" attr-message="Es un campo requerido!"><option value="-1">Ninguna</option></select>
        <div class="validation-error"><p></p></div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="col-md-8">            
            <div id="contenedor-form" class="row">
                <div class="col-md-12">
                    <div class="box bac">
                        <div class="box-header">
                            <h3 class="titulo">Formulario Atención</h3>                            
                        </div>
                        <div class="box-body">
                            <div class="nav-tabs-custom tab-danger">
                                <!-- Nav tabs -->
                                <ul class="nav nav-tabs" role="tablist">                                                                        
                                    <li role="presentation" class="active"><a href="#ver_cita" aria-controls="ver_cita" role="tab" data-toggle="tab">Información Cliente</a></li>
                                    <li role="presentation"><a href="#nueva_cita" aria-controls="nueva_cita" role="tab" data-toggle="tab">Resolución</a></li>
                                    <li role="presentation"><a href="#mas_tarjetas" aria-controls="mas_tarjetas" role="tab" data-toggle="tab">Más Tarjetas</a></li>
                                </ul>
                                <!-- Tab panes -->
                                <div class="tab-content bac-table-panelx">
                                    <div role="tabpanel" class="tab-pane fade in active" id="ver_cita">                                  
                                        <div class="row no-margin">
                                            <div class="col-xs-12">
                                                <div class="form-group input-group-sm" id="div_identificacion">
                                                    <label for="txt_identificacion">Identificacion</label>
                                                    <input type="text" class="form-control id limpiar input requerido" id="txt_identificacion" name="txt_identificacion" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-12">
                                                <div class="form-group input-group-sm" id="div_nombre">
                                                    <label for="txt_nombre">Nombre</label>
                                                    <input type="text" class="form-control nombre limpiar input requerido" id="txt_nombre" name="txt_nombre" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-12 col-sm-6">
                                                <div class="form-group input-group-sm" id="div_email">
                                                    <label for="txt_email">Email</label>
                                                    <input type="text" class="form-control nombre limpiar input requerido" id="txt_email" name="txt_email" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                            <div class="col-sm-6">
                                                <div class="form-group input-group-sm" id="div_cel">
                                                    <label for="txt_cel">Celular</label>
                                                    <input type="text" class="form-control telefono limpiar input requerido" id="txt_cel" name="txt_cel" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row no-margin">
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tel_oficina">
                                                    <label for="txt_tel_oficina">Tel. Oficina</label>
                                                    <input type="text" class="form-control limpiar telefono input requerido" id="txt_tel_oficina" name="txt_tel_oficina" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tel_casa">
                                                    <label for="txt_tel_casa">Tel. Casa</label>
                                                    <input type="text" class="form-control telefono limpiar input requerido" id="txt_tel_casa" name="txt_tel_casa" placeholder="N/D" maxlength="50" autocomplete="off" disabled>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="row no-margin">
                                            <div class="col-xs-6">
                                                <div class="form-group input-group-sm" id="div_tarjeta">
                                                    <label for="txt_tarjeta">Tarjeta</label>
